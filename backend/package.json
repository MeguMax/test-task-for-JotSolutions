    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1"
  }
}
